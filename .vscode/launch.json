{
    // Use IntelliSense to learn about possible attributes.
    // Hover to view descriptions of existing attributes.
    // For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Launch Classification",
            "type": "python",
            "request": "launch",
            "program": "${file}",
            "console": "integratedTerminal",
            //"cwd": "${workspaceFolder}"
            "cwd": "${fileDirname}",
            "justMyCode": false,
            "args":
            [
                "--batch_size", "8",
                "--lr", "0.0001",
                "--epochs", "100",
                "--train_split", "0.8",
                "--path_dset", "../datasets/dataset_downsampled/",
                "--stations", "['Buelenberg_1']", // "['Buelenberg_1', 'Buelenberg_2', 'Giementaelli_1', 'Giementaelli_2', 'Giementaelli_3', 'Luksch_1', 'Luksch_2', 'Sattel_1', 'Sattel_2', 'Sattel_3', 'Stillberg_1', 'Stillberg_2', 'Stillberg_3']", // use one or all, otherwise baseline won't be found // comma separated, single elements in single string quotes, station_cam separated with underscore !
                "--test_on_dset", "['Buelenberg_1']", // max one ele in list !!! - this variable is to make sure a model can be tested on single cam if trained on all cams - if None, test on same set as model was trained
                "--weighted", "Auto", //Manual, Auto, False
                "--model", "resnet", //resnet, efficientnet, densenet, vgg11, googlenet
                "--optim", "SGD", // Adam, SGD
                "--lr_scheduler", "15", //either integer or "None"
                "--weight_decay", "0.1", // used for both, SGD and Adam optimizer, 0.1
                "--momentum", "0.8", // used for SGD optimizer, 0.9
            ]
        },
        {
            "name": "Launch Segmentation",
            "type": "python",
            "request": "launch",
            "program": "${file}",
            "console": "integratedTerminal",
            //"cwd": "${workspaceFolder}"
            "cwd": "${fileDirname}",
            "justMyCode": false,
            "args":
            [
                "--batch_size", "4",
                "--lr", "0.00001", // 0.00001
                "--epochs", "150",
                "--train_split", "0.8",
                "--path_dset", "/net/pf-pc20/scratch2/jbaumer/2022_MasterThesis/datasets/dataset_complete/", // "../datasets/dataset_complete/",
                "--optim", "Adam", // Adam, SGD
                "--lr_scheduler", "7", //either integer or "None"
                "--weight_decay", "0.1", // used for both, SGD and Adam optimizer, 0.1
                "--momentum", "0.9", // used for SGD optimizer, 0.9
                "--stations", "['Buelenberg_2']",  //comma separated, single elements in string quotes, station_cam separated with underscore !
            ],
            "env": 
                {
                "PYDEVD_WARN_EVALUATION_TIMEOUT" : "10"
                }
        },
        {
            "name": "Launch Baseline Classification",
            "type": "python",
            "request": "launch",
            "program": "${file}",
            "console": "integratedTerminal",
            //"cwd": "${workspaceFolder}"
            "cwd": "${fileDirname}",
            "justMyCode": false,
            "args":
            [
                "--stations", "['Stillberg_3']", //, 'Buelenberg_2', 'Giementaelli_1', 'Giementaelli_2', 'Giementaelli_3', 'Luksch_1', 'Luksch_2', 'Sattel_1', 'Sattel_2', 'Sattel_3', 'Stillberg_1', 'Stillberg_2', 'Stillberg_3']",
                "--path_dset", "../datasets/dataset_downsampled/"
            ]
        },
        {
            "name": "Launch Baseline Segmentation",
            "type": "python",
            "request": "launch",
            "program": "${file}",
            "console": "integratedTerminal",
            //"cwd": "${workspaceFolder}"
            "cwd": "${fileDirname}",
            "justMyCode": false,
            "args":
            [
                "--stations", "['Buelenberg_2']", //, 'Buelenberg_2', 'Giementaelli_1', 'Giementaelli_2', 'Giementaelli_3', 'Luksch_1', 'Luksch_2', 'Sattel_1', 'Sattel_2', 'Sattel_3', 'Stillberg_1', 'Stillberg_2', 'Stillberg_3']",
                "--path_dset", "/net/pf-pc20/scratch2/jbaumer/2022_MasterThesis/datasets/dataset_complete/"
            ]
        },
        {
            "name": "Launch any other file",
            "type": "python",
            "request": "launch",
            "program": "${file}",
            "console": "integratedTerminal",
            "cwd": "${fileDirname}",
        }
    ]
}
